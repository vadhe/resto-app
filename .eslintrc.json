{
    "env": {
        "browser": true,
        "es2021": true,
        "jasmine": true,
        "codeceptjs/codeceptjs": true
    },
    "extends": [
        "google",
        "plugin:codeceptjs/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "require-jsdoc" : 0,
        "max-len": 0,
        "new-cap": 0
    },
    "plugins": ["jasmine", "codeceptjs"]
}
